//Constructors
class Student {
  //default constructor
  /*
  Student(){
  }
  */
  int id;
  String name;

  //Parametrized Constructor
  /*Student (int id, String name)
  {
    this.id=id;
    this.name=name;
  }*/
  Student(this.id, this.name);

  Student.myCustomConstructor(this.id,this.name);
  void sleep()
  {
    print("zzz");
  }
}



void main()
{
  Student S1= Student(1,"Lior");
  Student S2= Student.myCustomConstructor(54, "John");
}
