void main()
{
  //Using BREAK
  for(int i=1;i<10;i++)
    {
      if (i==6)
      break;
     print (i);
    }
  //Using Labels
  outerLoop: for(int t=1;t<=3;t++)
    {
      innerLoop:for(int j=1;j<=3;j++)
        {
          print("$t $j");
          if(t==2 && j==2)
            {
              break outerLoop;
            }
        }
    }
}
